{
  "manifest_version": 3,
  "name": "DiscoDig",
  "description": "Get fun analytics from your Discord DMs and GCs!",
  "version": "1.1",
  "icons": {
    "128": "assets/imgs/logo128.png",
    "64": "assets/imgs/logo48.png",
    "16": "assets/imgs/logo16.png"
  },
  "content_scripts": [
    {
      "js": [
        "libs/plotly.js", "libs/wordcloud2.min.js", "content.js"
      ],
      "matches": [
        "https://*.discord.com/channels/@me/*"
      ],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
  {
    "resources": ["content.js.map", "libs/plotly.js", "libs/wordcloud2.min.js", "assets/*"],
    "matches": ["<all_urls>"]
  }]
  
}